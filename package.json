{
  "name": "@ctrl/qbittorrent",
  "version": "0.0.0-placeholder",
  "description": "TypeScript api wrapper for qbittorrent using got",
  "author": "Scott Cooper <scttcper@gmail.com>",
  "license": "MIT",
  "repository": "typectrl/qbittorrent",
  "main": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "typescript",
    "qbittorrent"
  ],
  "scripts": {
    "lint": "eslint --ext .js,.ts, .",
    "lint:fix": "eslint --fix --ext .js,.ts, .",
    "prepare": "npm run build",
    "build": "tsc -p tsconfig.build.json",
    "build:docs": "typedoc --out docs --hideGenerator --target ES6 --mode file src && touch docs/.nojekyll",
    "test": "jest --runInBand",
    "test:watch": "jest --watch --runInBand",
    "test:ci": "jest --ci --reporters=default --reporters=jest-junit --runInBand --coverage",
    "semantic-release": "semantic-release"
  },
  "dependencies": {
    "@ctrl/shared-torrent": "^3.0.0",
    "@ctrl/torrent-file": "^1.2.0",
    "form-data": "^3.0.0",
    "got": "^11.0.2",
    "tough-cookie": "^4.0.0",
    "url-join": "^4.0.1"
  },
  "devDependencies": {
    "@types/form-data": "2.5.0",
    "@types/jest": "25.2.1",
    "@types/node": "13.13.4",
    "@types/tough-cookie": "4.0.0",
    "@types/url-join": "4.0.0",
    "@typescript-eslint/eslint-plugin": "2.29.0",
    "@typescript-eslint/parser": "2.29.0",
    "eslint": "6.8.0",
    "eslint-config-xo-space": "0.24.0",
    "eslint-config-xo-typescript": "0.27.0",
    "eslint-plugin-import": "2.20.2",
    "jest": "25.5.3",
    "jest-junit": "10.0.0",
    "p-wait-for": "3.1.0",
    "semantic-release": "17.0.7",
    "ts-jest": "25.4.0",
    "typedoc": "0.17.4",
    "typescript": "3.8.3"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node"
  },
  "release": {
    "branch": "master"
  },
  "engines": {
    "node": ">=10.19.0"
  }
}
